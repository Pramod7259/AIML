s1={10,20,30,40}
s2={40,60,30,80}
s3=set()
while(True):
    print("1.Union 2.intersection 3.diffrence 4.symmatric_diffrence 5.Exite")
    choice=int(input("Enter your choice:-"))
    if(choice==1):
        s3=s1.union(s2)
        print(s3)
    elif(choice==2):
        s3=s1.intersection(s2)
        print(s3)
    elif(choice==3):
        s3=s1-s2
        print(s3)
    elif(choice==4):
        s3=s1^s2
        print(s3)
    else:
        break;    