a={'car':'Briza','year':2016,'colour':'Red','cc':150}
print(a)
print("\n\n")
b=a.keys()
print("Only Keys is :-",b)
print("\n")
c=a.values()
print("Only values is :-",c)
print("\n")
d=a.items()
print("Items is :-",d)
print("\n")
e=a.get("car")
print("The Car name is :-",e)
print("\n")
f=a.copy()
print(f)
g=a.popitem()
print(g)
h=a.pop("colour")
print(h)
print(a)
print("\n\n")
i={1:'EC',2:'CS',3:'CP',4:'Civil'}
a.update(i)
print(a)
a.clear()
print(a)