class Node:
    def __init__(self, item):
        self.data = item
        self.left = None
        self.right = None

def inorder(root):
    if root:
        inorder(root.left) 
        print(root.data, end=" ")
        inorder(root.right)


print("The Binary Tree is:",end=" ")
root =Node(20) 
root.left =Node(15) 
root.right =Node(25)  
root.left.left =Node(10) 
root.left.right =Node(18) 
root.right.left =Node(22)  # Fixed line
root.right.right =Node(28)  

inorder(root)