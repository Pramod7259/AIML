def insertionsort(a): 
        n = len(a) 
        for i in range(1,n): 
               v=a[i] 
               j = i-1 
               while j>=0 and a[j]>v: 
                        a[j+1] = a[j] 
                        j=j-1 
                        a[j+1] = v 
x = [50,28,15,35,14,58] 
print("Before sorting:",x) 
insertionsort(x) 
print("After sorting:",x)