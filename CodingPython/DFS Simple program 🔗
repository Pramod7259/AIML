class Node:
    def __init__(self,item):
        self.data=item
        self.left = None
        self.right = None

def dfs(root):
    if root is None:
        return
    print(root.data,end=" ")
    dfs(root.left) 
    dfs(root.right)# Visit left
    # Visit node  # Visit right

print("DFS Traversal :",end=" ")
# Creating a simple BST
root = Node(20)
root.left = Node(15)
root.right = Node(25)
root.left.left = Node(10)
root.left.right = Node(18)
root.right.left=Node(22)
root.right.right=Node(28)

dfs(root)